{
  "name": "react-localize-redux",
  "version": "4.0.0-beta.0",
  "description": "Localization library for handling translations in React",
  "main": "./lib/index.js",
  "module": "./es/index.js",
  "jsnext:main": "./es/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "coverage": "jest && codecov",
    "prepublish": "npm run build",
    "test": "jest",
    "prettier": "prettier --write src/**/*",
    "upload:demo": "codesandbox ./demo"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ryandrewjohnson/react-localize-redux.git"
  },
  "keywords": [
    "react",
    "redux",
    "intl",
    "i18n",
    "internationalization",
    "locale",
    "localization",
    "globalization"
  ],
  "tags": [
    "react",
    "redux",
    "i18n",
    "localization"
  ],
  "author": "Ryan Johnson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ryandrewjohnson/react-localize-redux/issues"
  },
  "homepage": "https://github.com/ryandrewjohnson/react-localize-redux#readme",
  "peerDependencies": {
    "react": "^16.8.0"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.18",
    "@types/flat": "0.0.28",
    "@types/hoist-non-react-statics": "^3.0.1",
    "@types/jest": "^24.0.6",
    "@types/react": "^16.8.3",
    "@types/reselect": "^2.2.0",
    "codecov": "^3.0.2",
    "codesandbox": "^1.2.10",
    "cross-env": "^3.1.4",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "immutable": "^3.8.2",
    "jest": "^24.1.0",
    "jest-dom": "^3.0.2",
    "json-loader": "^0.5.4",
    "prettier": "^1.16.4",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "react-testing-library": "^5.9.0",
    "redux": "^3.6.0",
    "rollup": "^1.2.1",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-typescript": "^1.0.0",
    "ts-jest": "^24.0.0",
    "ts-node": "^7.0.1",
    "tslib": "^1.9.3",
    "typescript": "^3.3.3"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "roots": [
      "tests/"
    ],
    "modulePaths": [
      "src/",
      "node_modules"
    ],
    "setupFiles": [
      "<rootDir>/jest.setup.js"
    ]
  },
  "dependencies": {
    "flat": "^4.1.0",
    "hoist-non-react-statics": "^3.3.0",
    "prop-types": "^15.6.1",
    "reselect": "^4.0.0"
  }
}
